   "source": [
    "import glob\n",
    "import os\n",
    "from IPython.display import display, HTML\n",
    "\n",
    "for f in glob.glob('*.ipynb'):\n",
    "    if 'index' in f:\n",
    "        continue\n",
    "    \n",
    "    fname, _ = os.path.splitext(f)\n",
    "    date, name = fname.split('_')\n",
    "    display(HTML('<a href=\"{}\">{}: {}'.format(f, date, name)))"
   ]
  }
